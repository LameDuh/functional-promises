{
  "name": "functional-promises",
  "version": "1.8.0",
  "description": "Fluent Composition Pipeline Promises API. See Docs: https://fpromises.io/",
  "esm": "auto",
  "ava": {
    "files": [
      "tests/**/*.js",
      "!tests/data/**"
    ],
    "require": [
      "babel-register"
    ],
    "babel": {
      "presets": [
        [
          "env",
          {
            "debug": false,
            "modules": false,
            "useBuiltIns": true,
            "targets": {
              "node": "6"
            }
          }
        ]
      ]
    }
  },
  "typings": "./index.d.ts",
  "main": "./dist/functional-promise.min.js",
  "module": "./src/index.js",
  "browser": "./dist/functional-promise.min.js",
  "unpkg": "./dist/functional-promise.min.js",
  "repository": {
    "url": "https://github.com/functional-promises/functional-promises.git"
  },
  "homepage": "https://www.fpromises.io/",
  "scripts": {
    "start": "npm run build && rollup -c --watch",
    "test": "npm run build && ava --serial --verbose ",
    "docs-docker": "cd docs-source && docker build -t fpromises-docs:latest .",
    "docs-build": "./docs-source/docker-build.sh",
    "build": "npm run build-dev && npm run build-live",
    "build-live": "NODE_ENV=production rollup -c",
    "build-dev": "NODE_ENV=development rollup -c",
    "build-pack": "prepack ./dist/bundle.js --out ./dist/bundle.prepack.js  ",
    "deploy": "npm run build && npm run docs-build && npm run test && npm run publish",
    "version-bump-revision": "node ./misc/package-version-manager.js 0.0.1",
    "version-bump-minor": "node ./misc/package-version-manager.js 0.1.0"
  },
  "author": {
    "name": "Dan Levy",
    "email": "Dan@DanLevy.net",
    "url": "http://www.danlevy.net/"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "ava": "^0.25.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "chalk": "^2.4.1",
    "css-loader": "^0.28.11",
    "debug": "^3.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.9.1",
    "eslint": "^5.7.0",
    "jsdom": "^13.1.0",
    "rollup-loader": "^0.3.0",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-commonjs": "^8.4.1",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-size-snapshot": "^0.7.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup": "^0.59.1",
    "strip-loader": "^0.1.2",
    "tape": "^4.9.0"
  }
}
